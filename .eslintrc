{
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "plugin:import/react"
  ],
  "plugins": [
    "prettier",
    "react"
  ],
  "rules": {
    "react/prop-types": 0,
    "prettier/prettier": "error",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}